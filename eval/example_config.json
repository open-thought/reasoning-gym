{
  "model": "meta-llama/llama-3.3-70b-instruct",
  "provider": "Hyperbolic",
  "output_dir": "results",
  "max_concurrent": 10,
  "default_size": 50,
  "default_seed": 42,
  "categories": [
    {
      "category": "algebra",
      "datasets": [
        {
          "dataset": "complex_arithmetic",
          "params": {
            "min_size": 3,
            "max_size": 5
          }
        },
        {
          "dataset": "caesar_cipher",
          "size": 30,
          "seed": 123
        }
      ]
    },
    {
      "category": "arithmetic",
      "datasets": [
        {
          "dataset": "chain_sum",
          "size": 100,
          "seed": 789,
          "params": {
            "min_terms": 2,
            "max_terms": 5,
            "min_digits": 1,
            "max_digits": 3
          }
        }
      ]
    }
  ]
}
